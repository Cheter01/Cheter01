---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Cheter01's Blog</title>
	<link href="https://cheter01.github.io/cheter01/blog/atom.xml" rel="self"/>
	<link href="https://cheter01.github.io/cheter01/blog/"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://cheter01.github.io/cheter01/blog/</id>
	<author>
		<name>Cheter01</name>
		<email>lorenzo.chesi@live.it</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://cheter01.github.io/cheter01/{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://cheter01.github.io/cheter01/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
